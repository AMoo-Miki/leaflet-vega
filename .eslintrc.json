{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "root": true,
  "env": {
    "commonjs": true,
    "amd": true,
    "node": false
  },
  "extends": "mourner",
  "rules": {
    "space-before-function-paren": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      "error",
      "backtick"
    ]
  }
}
